/**
 * Copyright by Rittler & Co
 **/

var init = false;
var mobile = false;

if(typeof jQuery.fn.andSelf !== "function") {
	jQuery.fn.andSelf = function() {
	  return this.addBack.apply(this, arguments);
	}
}
jQuery(document).ready(function ($) {

    $('.ce_rsce_service .headline').click(function () {
        $(this).parent('.service').toggleClass('active');
        $(this).siblings('.content').slideToggle();
    })


    $('body').removeClass('loading').addClass('loaded');

    // mobile

    if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
        var mobile = true;
        $('body').addClass('showResponsiveMenu');
    }

    var $mmenu = $('<div>').addClass('mobile_menu');
    var $menu =$('.nav_main').clone();
    $menu.find('li.submenu').append('<span class="trigger"></span>');
    $menu.find('li.submenu.trail, li.submenu.active').addClass('show_submenu');
    $mmenu.append($menu);
    var $footer = $('#footer .footer_top').clone().addClass('footer');
    $mmenu.append($footer);
    $mmenu.wrapInner('<div class="inside">');
    $('body').append($mmenu);

    $mmenu.find('.trigger').click(function(){
        $(this).parent().toggleClass('show_submenu');
    })

    $('.menu-trigger').click(function () {
        $('body').toggleClass('active-menu');
    })


    if(!mobile) {

        var $fixedHeader = $('#header').clone();
        $fixedHeader.addClass('fixed');
        $fixedHeader.find('.tel, .ce_rsce_partner_slider, .fb').remove();
        $fixedHeader.find('.header_top').contents().unwrap();
        $fixedHeader.find('.header_bottom').contents().unwrap();
        $fixedHeader.insertAfter($('#header'));
        
        $(window).scroll(function(){

            var scrollTop = $(window).scrollTop();

            if(scrollTop > 200) {
                $('body').addClass('showFixedHeader');
            } else {
                $('body').removeClass('showFixedHeader');
            }

        })


    }

    // owl

    $('.ce_rsce_partner_slider .slides').owlCarousel({
        loop: true,
        autoWidth: true,
        nav: true,
        dots: false,
        margin: 20
    })

    if($('.ce_rsce_home_slider').length) {

        $('.ce_rsce_home_slider .slides_top').owlCarousel({
            items: 1,
            dots: true
        })

        $('.ce_rsce_home_slider .dot_subline').appendTo($('.ce_rsce_home_slider .slides_top .owl-controls'));


        $('.ce_rsce_home_slider .slides_bottom').owlCarousel({
            items: 3,
            loop: true,
            nav: true,
            dots: false,
            responsive: {
                0: {
                    items: 1
                },
                600: {
                    items: 2
                },
                900: {
                    items: 3
                }
            }
        }).on('translate.owl.carousel', function (e) {
            var $owl = $(e.target);
            var $item = $owl.find('.owl-item.active:first');
            $item.siblings().removeClass('aactive');
        }).on('translated.owl.carousel', function (e) {
            var $owl = $(e.target);
            var $item = $owl.find('.owl-item.active:first');
            $item.siblings().removeClass('aactive');
            $item.addClass('aactive');
        });

        $('.ce_rsce_home_slider .slides_bottom .owl-item.active:first').addClass('aactive');

    }

    if($('.ce_rsce_product_offer').length) {
        var $dots = $('.ce_rsce_product_offer .dots');
        setInterval(function(){

            var $activeDot = $dots.find('.dot.active');

            if(!$activeDot.length) {
                $activeDot = $dots.find('.dot:first').addClass('active');
            }

            var $nextDot = $activeDot.next();

            $activeDot.removeClass('active');

            if(!$nextDot.length) {
                $nextDot = $dots.find('.dot:first');
            }

            setTimeout(function(){
                $nextDot.addClass('active');
            }, 200)



        }, 800)
    }


    $('.ce_rsce_teasers .slides').owlCarousel({
        navRewind: false,
        nav: false,
        dots: false,
        margin: 70,
        responsiveClass:true,
        mouseDrag: true,
        touchDrag: true,
        pullDrag: true,
        responsive: {
            0: {
                items: 1,
                dots: true,
                nav: true,
                mouseDrag: true,
                touchDrag: true,
                pullDrag: true
            },
            600: {
                items: 2,
                dots: true,
                nav: true,
                mouseDrag: true,
                touchDrag: true,
                pullDrag: true
            },
            900: {
                items: 3,
                mouseDrag: false,
                touchDrag: false,
                pullDrag: false
            }
        }
    })

    $('.ce_gallery ul').owlCarousel({
        items: 1,
        nav: true,
        dots: true,
        // stagePadding: 50,
        loop: true
    })

    $("a.yt").colorbox({
        iframe: true,
        innerWidth: 640,
        innerHeight: 390,
        maxWidth: '95%',
        maxHeight: '95%'
    });


    if($('.ce_rsce_product_intro').length) {
        $('.nav_breadcrumb').hide();
    }


    // form


    $('select').select2({});

    $('input').icheck({
        checkboxClass: 'icheckbox_minimal-grey icheckbox',
        radioClass: 'iradio_minimal-grey iradio',
        increaseArea: '20%' // optional
    });

    $('.checkbox_container .icheckbox, .radio_container .iradio').closest('span').addClass('icheck_container');


    $('.ce_form input, .ce_form textarea').each(function (index, el) {

        var $el = $(el);
        var $widget = $el.closest('.widget');

        if ($widget.hasClass('widget-upload')) {
            return;
        }

        var $label = $widget.find('> label');

        if (!$label.length) {
            return;
        }

        var $labelCp = $label.clone();
        $labelCp.find('.invisible').remove();

        var label = $.trim($labelCp.text());

        if ($el.hasClass('captcha')) {
            return;
        }

        if (label.length) {
            $label.addClass('float-label');
            // $el.attr('placeholder', label);

            $el.on('focus', function () {
                $widget.addClass('widget-focus');
            })

            $el.on('blur', function () {
                $widget.removeClass('widget-focus');
            })

            $el.on('input', function () {

                var val = $(this).val();
                if (val.length) {
                    $widget.addClass('float-label-active');
                    if (init) {
                        $widget.find('p.error').fadeOut();
                    }
                } else {
                    $widget.removeClass('float-label-active');
                }

            }).trigger('input');
        }

    })



    $(window).resize(function () {
        onResize();
    }).resize();

    function onResize() {

        if (!mobile) {

            if ($(window).width() < 900 || $(window).height() < 500) {
                $('body').addClass('showResponsiveMenu');
            } else {
                $('body').removeClass('showResponsiveMenu');
            }

        }

        if ($('.ce_rsce_home_slider').length) {

            var h = $(window).height() - $('#header').height()

            if($('body').hasClass('showResponsiveMenu')) {
                h = 500;
            }

            if(h < 500) {
                h = 500;
            }

            $('.ce_rsce_home_slider .slides_top .slide').css({
                height: h
            })

            $.each($('.ce_rsce_home_slider .slide'), function(index, el) {

                var $slide = $(el);
                var textHeight = $slide.find('.text_container').height();
                var marginTop = h / 2 - textHeight / 2;

                marginTop -= 50; // dot navigation

                if($('body').hasClass('showResponsiveMenu')) {
                    marginTop = 50;
                }

                $slide.find('.text_container').css({
                    top: marginTop
                })


            });



        }

    }

    init = true;

});


// Google Analytics Tracking (Markus)

var google_tracking = {

    init: function() {

        google_tracking.events();
    },

    events: function() {

        $("header#header .header_top .tel a").on("click", function(ev) {
            
            google_tracking.track("phone");
        });
    },

    track: function(caption) {

        console.log(caption);

        ga("send", "event", "contact", caption);
        goog_report_conversion("tel:+43316711486"); // Google Adwords Tracking
    }
};

$(document).ready(function() {

    google_tracking.init();
});

